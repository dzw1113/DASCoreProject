<?xml version="1.0"?>
<configuration scan="true" scanPeriod="30 seconds">
	<contextName>DAS</contextName>  
	<appender name="A1" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-4r [%t] %-5p %c - %m%n</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="A1" />
	</root>
</configuration>
